<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_russian" class="com.google.android.apps.inputmethod.libs.latin.LatinIme" language="ru" auto_capital="true" display_app_completions="true">
        <keyboard_group variant="russian">
            <keyboard type="prime" def="@xml/keyboard_ru" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="russian_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_ru">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_11_11_11" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>