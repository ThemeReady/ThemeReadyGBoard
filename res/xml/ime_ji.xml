<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_yiddish" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="ji" auto_capital="false" display_app_completions="true">
        <keyboard_group variant="yiddish">
            <keyboard type="prime" def="@xml/keyboard_ji" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="yiddish_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_ji">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_10_10_10_no_shift" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>