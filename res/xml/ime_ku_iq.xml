        <keyboard_group variant="kurdish">
            <keyboard def="@xml/keyboard_ku_iq" type="prime" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="kurdish_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_ku_iq">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_11_11_11_no_shift" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>