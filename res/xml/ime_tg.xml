<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_tajik" class="com.google.android.apps.inputmethod.libs.latin.LatinIme" language="tg" auto_capital="true" display_app_completions="true">
        <keyboard_group variant="tajik">
            <keyboard type="prime" def="@xml/keyboard_tg" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="tajik_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_tg">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_12_11_11" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>