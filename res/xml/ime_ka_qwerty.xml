<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_georgian_qwerty" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="ka" auto_capital="false" display_app_completions="true">
        <keyboard_group variant="georgian_qwerty">
            <keyboard type="prime" def="@xml/keyboard_ka_qwerty" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="georgian_qwerty_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_ka_qwerty">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_10_9_9" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>