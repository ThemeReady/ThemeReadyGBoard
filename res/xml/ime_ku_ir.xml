<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_kurdish_iran" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="ku_IR" auto_capital="false" display_app_completions="true">
        <keyboard_group variant="kurdish">
            <keyboard type="prime" def="@xml/keyboard_ku_ir" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="persian_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_ku_ir">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_11_11_11_no_shift" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>