<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_macedonian" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="mk" auto_capital="true" display_app_completions="true">
        <keyboard_group variant="macedonian">
            <keyboard type="prime" def="@xml/keyboard_mk" />
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
        <keyboard_group variant="macedonian_with_numbers" keyboard_height_ratio="@dimen/keyboard_height_ratio_5_rows">
            <keyboard type="prime" def="@xml/keyboard_mk">
                <merge def="@xml/keyboard_latin_fragment_numbers_10_11_11_11" />
            </keyboard>
            <include href="@xml/keyboards_non_prime_latin" />
        </keyboard_group>
    </ime>
</framework>